<div class="top-wrapper">
  <div class="container">
    <div><h1>Log in</h1></div><br>
      <%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
        <%= f.label :name %><br>
        <%= f.text_field :name %><br>
          
        <%= f.label :email %><br>
        <%= f.email_field :email %><br>
          
        <%= f.label :password %><br>
        <%= f.password_field :password %><br><br>
          
        <%= f.submit "Log in" %>
      <% end %>
  </div>
</div>
    
  